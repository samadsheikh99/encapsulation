class Person:
    def __init__(self, name, age, address):
        self._name = name
        self._age = age
        self._address = address

    def get_name(self):
        return self._name

    def set_name(self, name):
        self._name = name

    def get_age(self):
        return self._age

    def set_age(self, age):
        self._age = age

    def get_address(self):
        return self._address

    def set_address(self, address):
        self._address = address

        # Creating a magical being named Alice
alice = Person(name="Alice", age=25, address="Wonderland")

# Displaying the attributes using the 'get' spells
print(f"Name: {alice.get_name()}")
print(f"Age: {alice.get_age()}")
print(f"Address: {alice.get_address()}")

# Modifying attributes with the 'set' spells
alice.set_name("Alicia")
alice.set_age(26)
alice.set_address("Magical Forest")

# Displaying the updated attributes
print(f"\nUpdated Name: {alice.get_name()}")
print(f"Updated Age: {alice.get_age()}")
print(f"Updated Address: {alice.get_address()}")
